{
  "name": "admin-front",
  "author": "bee",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:validation": "CI=true yarn test --coverage --passWithNoTests",
    "lint": "eslint src/",
    "format": "prettier --write \"**/*.+(js|json|css|md|mdx|html)\"",
    "validate": "npm-run-all --parallel format lint test:validation",
    "setup": "yarn && yarn run validate",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "polished": "^3.6.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-scripts": "3.4.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@jackfranklin/test-data-bot": "^1.3.0",
    "@testing-library/dom": "^7.20.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^10.4.4",
    "@testing-library/user-event": "^12.0.11",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "eslint-plugin-testing-library": "^3.3.1",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "engines": {
    "node": ">=14.4.0"
  }
}
